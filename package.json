{
  "name": "stellarexplorer",
  "description": "Ledger explorer for the Stellar network",
  "version": "2.7.1",
  "license": "Apache-2.0",
  "author": "Chris Hatch <devcebfaff@proton.me>",
  "homepage": "https://steexp.com",
  "repository": "chatch/stellarexplorer",
  "bugs": {
    "url": "https://github.com/chatch/stellarexplorer/issues"
  },
  "keywords": [
    "stellar",
    "react",
    "ledger",
    "soroban",
    "smart contract",
    "remix.run",
    "block explorer",
    "blockchain"
  ],
  "engines": {
    "node": ">= 20.x.x"
  },
  "sideEffects": false,
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "vite preview",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix . || true",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "fixstyle": "pnpm prettier:fix",
    "fixall": "npm-run-all prettier:fix lint:fix",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.17.2",
    "@remix-run/node": "2.17.2",
    "@remix-run/react": "2.17.2",
    "@remix-run/server-runtime": "2.17.2",
    "@sentry/remix": "7.109.0",
    "@stellar/js-xdr": "3.0.1",
    "@stellar/stellar-sdk": "14.3.2",
    "bignumber.js": "^9.1.1",
    "bootstrap": "^5.3.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "p-retry": "^6.2.0",
    "prismjs": "^1.29.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "2.8.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "react-json-pretty": "^2.2.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.21.1",
    "urijs": "1.19.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@flydotio/dockerfile": "^0.4.7",
    "@playwright/test": "^1.40.0",
    "@remix-run/dev": "2.17.2",
    "@remix-run/eslint-config": "2.17.2",
    "@remix-run/testing": "2.4.1",
    "@swc/core": "1.3.101",
    "@swc/jest": "0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "29.5.11",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.196",
    "@types/node": "20.10.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.2.45",
    "@types/react-bootstrap": "0.32.35",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.2.18",
    "@types/react-router-bootstrap": "0.26.6",
    "@types/react-test-renderer": "^18.0.7",
    "@types/urijs": "^1.19.25",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-preview": "^0.3.1",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "react-test-renderer": "^18.2.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.6",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0"
  }
}
